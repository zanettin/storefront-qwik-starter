{
	"name": "storefront-qwik-starter",
	"description": "File based routing and MDX support Vite.js tooling. Cloudflare Pages. Prettier code formatter. Tailwind CSS framework.",
	"scripts": {
		"build": "npm run typecheck && npm run build.client && npm run build.ssr",
		"build.client": "vite build",
		"build.server": "vite build -c adaptors/cloudflare-pages/vite.config.ts",
		"build.ssr": "vite build --ssr src/entry.cloudflare-pages.tsx",
		"dev": "npm run dev.ssr",
		"dev.client": "vite",
		"dev.debug": "node --inspect-brk node_modules/vite/bin/vite.js --force --mode ssr",
		"dev.ssr": "node --inspect node_modules/vite/bin/vite.js --mode ssr",
		"fmt": "prettier --write .",
		"fmt.check": "prettier --check .",
		"lint": "eslint \"src/**/*.ts*\"",
		"prepare": "husky install",
		"serve": "wrangler pages dev ./dist",
		"start": "npm run dev",
		"typecheck": "tsc --incremental --noEmit"
	},
	"devDependencies": {
		"@builder.io/qwik": "0.13.3",
		"@builder.io/qwik-city": "0.0.122",
		"@tailwindcss/forms": "^0.5.2",
		"@types/eslint": "8.4.10",
		"@types/node": "latest",
		"@typescript-eslint/eslint-plugin": "5.42.1",
		"@typescript-eslint/parser": "5.42.1",
		"autoprefixer": "10.4.13",
		"eslint": "8.27.0",
		"eslint-plugin-qwik": "0.13.3",
		"husky": "^8.0.2",
		"node-fetch": "3.3.0",
		"postcss": "8.4.19",
		"prettier": "2.7.1",
		"pretty-quick": "^3.1.3",
		"tailwindcss": "3.2.4",
		"typescript": "4.8.4",
		"vite": "3.2.3",
		"vite-tsconfig-paths": "3.5.2",
		"wrangler": "^2.2.1"
	},
	"engines": {
		"node": ">=14"
	},
	"homepage": "https://qwik.builder.io/",
	"license": "",
	"private": true,
	"type": "module"
}
